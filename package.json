{
  "name": "@jayson991/react-iconfont",
  "version": "1.0.2",
  "description": "A Project For React To Use Iconfont Intelligently",
  "author": "Jayson Wu <jaysonwu991@outlook.com>",
  "license": "MIT",
  "main": "lib/cjs/index.js",
  "module": "lib/esm/index.js",
  "types": "lib/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./lib/esm/index.js",
      "require": "./lib/cjs/index.js"
    }
  },
  "scripts": {
    "test": "jest",
    "build": "vite build",
    "build:types": "tsc --outDir lib/types",
    "build:esm": "swc src -C module.type=es6 -d lib/esm",
    "build:cjs": "swc src -C module.type=commonjs -d lib/cjs",
    "lint": "eslint ./ --ext .js,.tsx",
    "lint:fix": "eslint ./ --fix --ext .js,.tsx",
    "prettier": "prettier --write src/**/*.tsx"
  },
  "files": [
    "lib",
    "README.md",
    "package.json"
  ],
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "react",
    "@jayson991/react-iconfont"
  ],
  "homepage": "https://github.com/jaysonwu991/react-iconfont",
  "bugs": {
    "url": "https://github.com/jaysonwu991/react-iconfont/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jaysonwu991/react-iconfont.git"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.3.4",
    "@swc/jest": "^0.2.23",
    "@testing-library/react": "^13.4.0",
    "@types/jest": "^29.1.1",
    "@types/node": "^18.8.2",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.39.0",
    "@typescript-eslint/parser": "^5.39.0",
    "@vitejs/plugin-react": "^2.1.0",
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.8",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.1.2",
    "jest-environment-jsdom": "^29.1.2",
    "prettier": "^2.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4",
    "vite": "^3.1.4"
  },
  "peerDependencies": {
    "react": ">=16.8.0"
  }
}
